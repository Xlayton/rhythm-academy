{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/vue3-recommended",
        "@vue/typescript",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "warn",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}